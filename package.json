  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "axios": "^0.19.0",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "sort-json-array": "^0.1.7"
  }
}
